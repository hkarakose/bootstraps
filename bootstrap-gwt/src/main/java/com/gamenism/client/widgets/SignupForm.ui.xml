<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:g='urn:import:com.google.gwt.user.client.ui'
			 xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'
		>
	<g:HTMLPanel>
		<b:TextBox ui:field="email" blockLevel="true" placeholder="Email address"/>
		<b:PasswordTextBox ui:field="password" blockLevel="true" placeholder="Password"/>
		<b:PasswordTextBox ui:field="passwordAgain" blockLevel="true" placeholder="Password (Again)"/>
	</g:HTMLPanel>
</ui:UiBinder>